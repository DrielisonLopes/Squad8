<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Mulher_Presente</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link href="~/css/style.css" rel="stylesheet">
    <script src="~/js/main.js"></script>
     
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Raleway:300,300i,400,400i,500,500i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i"
    rel="stylesheet">
    
</head>
<body>
    <header>
       
         <nav class="nav-menu d-none d-lg-block">
        <ul>
          <li class=""><a  asp-area="" asp-controller="Home" asp-action="Index" >Home</a></li>
          <li><a  asp-area="" asp-controller="Home" asp-action="TiposViolencia" >Agressões</a></li>
          <li><a  asp-area="" asp-controller="Home" asp-action="Servicos">Serviços</a></li>
          <li><a  asp-area="" asp-controller="Vitima" asp-action="Index" >Mulher</a></li>
          <li><a  asp-area="" asp-controller="Parceiros" asp-action="Index">Parceria</a></li>
          <li><a  asp-area="" asp-controller="Home" asp-action="Contato">Contato</a></li>
        </ul>
        </nav>
      

    </header>

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

  
    
    

    <footer class="">
        <div class="container ">
            &copy; 2022 - Mulher Presente 
        </div>
    </footer>

    

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
